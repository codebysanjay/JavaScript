<%- include("partials/header"); -%> 

<h1>Compose</h1>
<form action="/compose" method="POST">
    <div class="form-group">
        <label for="composeTitle">Title</label>
        <input type="text" name="composeTitle" class="form-control">
    </div>
    <div class="form-group">
        <label for="composeContent">Post</label>
        <textarea name="composeContent" class="form-control"  cols="30" rows="5"></textarea>
    </div>
    
    
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer"); -%> 
